<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Yandex Tests">
    <parameter name="username" value="selenium.test.account"/>
    <parameter name="password" value="SeleniumTestAccount"/>

    <listeners>
        <listener class-name="com.coherensolutions.traning.automation.java.web.urnezaite.ITestListenerClass"/>
    </listeners>
    <test name="Log In Test">
        <classes>
            <class name="com.coherensolutions.traning.automation.java.web.urnezaite.LogInTest"/>
        </classes>
    </test>
    <test name="Log Out Test">
        <classes>
            <class name="com.coherensolutions.traning.automation.java.web.urnezaite.LogOutTest"/>
        </classes>
    </test>
</suite>